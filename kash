public class Newarray{
    public static void main(String[] args){
        int []a = {123,456,321,678};
        int sum=0;
        for(int i=0;i<a.length;i++){
            int temp=a[i];
             while(temp>0){
            int dig =temp%10;
            sum = sum+dig;
            temp=temp/10;
            }
        }
        System.out.println("Sum of digits: " + sum);
    }
}
